{
  "compilerOptions": {
      "jsx": "react-jsx",
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],      
      "allowJs": true,      
      "module": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "outDir": "./dist"
  },
  "include": [
    "src/framework"
  ]
}
